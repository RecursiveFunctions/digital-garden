Processing note: /java/prepared-statement/ {
  title: undefined,
  fileSlug: 'PreparedStatement',
  graphExclude: undefined,
  frontMatterContent: '\n' +
    '```java\n' +
    '\n' +
    '// Illustrates the use of  PreparedStatement for a repeated update.  \n' +
    '// This version just shows the syntax.  No repetition.  \n' +
    '\n' +
    '  \n' +
    '\n' +
    'import java.io.*;  \n' +
    '\n' +
    'import java.sql.*;  \n' +
    '\n' +
    'public class jdbc7Ora {  \n' +
    '\n' +
    '  public static void main(String [] aa) {  \n' +
    '\n' +
    '    String url;  \n' +
    '\n' +
    '    url = "jdbc:oracle:thin:@cslabdb:1525:cfedb";  \n' +
    '\n' +
    "        // jdbc is 'protocol', thin is the driver  ',  \n" +
    '\n' +
    "                  // and cs514 is the 'user data source'  \n" +
    '\n' +
    '    Connection conn;  \n' +
    '\n' +
    '    try { // invoke oracle thin driver; register it with DriverManager  \n' +
    '\n' +
    '      Class.forName("oracle.jdbc.driver.OracleDriver");  \n' +
    '\n' +
    '    }  \n' +
    '\n' +
    '    catch (Exception e) {  \n' +
    '\n' +
    '      System.out.println("MR.UnitSitQueries.constructor.Exception: " +  \n' +
    '\n' +
    '        e);  \n' +
    '\n' +
    '    }  \n' +
    '\n' +
    '    try {  \n' +
    '\n' +
    '      conn = DriverManager.getConnection(url,"eckberg","carl");  \n' +
    '\n' +
    '        // establish connection to DBMS or database  \n' +
    '\n' +
    '      PreparedStatement pstmt = conn.prepareStatement(  \n' +
    '\n' +
    '        "INSERT INTO deptbb02 VALUES(?,?,?,?,?)"); // creates object  \n' +
    '\n' +
    '          //  from which SQL commands  can be sent to the DBMS  \n' +
    '\n' +
    '      String dname,restaurant,location,motto;  \n' +
    '\n' +
    '      int deptno;  \n' +
    '\n' +
    '      dname = "snacks"; restaurant = "Jasmine"; location = "Keokuk";  \n' +
    '\n' +
    '      deptno = 70; motto = "Nice guys finish last";  \n' +
    '\n' +
    '      pstmt.setString(2,dname); pstmt.setString(3,restaurant);  \n' +
    '\n' +
    '      pstmt.setString(4,location); pstmt.setInt(1,deptno);  \n' +
    '\n' +
    '      pstmt.setString(5,motto);  \n' +
    '\n' +
    '      System.out.println("still circling Moose Jaw");  \n' +
    '\n' +
    '      pstmt.executeUpdate();  \n' +
    '\n' +
    '      pstmt.close();  \n' +
    '\n' +
    '      conn.close();  \n' +
    '\n' +
    '    }  \n' +
    '\n' +
    '    catch (SQLException e){System.err.println("OOPS " + e.getMessage());}  \n' +
    '\n' +
    '  }  \n' +
    '\n' +
    '}\n' +
    '```'
}
