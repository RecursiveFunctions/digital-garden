    '      // Try to find the file by alias or by searching through the file structure\n' +
    '      const startPath = "./src/site/notes/";\n' +
    '      let foundFilePath = null;\n' +
    '      \n' +
    '      try {\n' +
    '        const files = fs.readdirSync(startPath, { recursive: true });\n' +
    '        \n' +
    '        // First try to find a direct match by filename\n' +
    '        const exactMatch = files.find(file => {\n' +
    '          return file.toLowerCase() === `${fileLink.toLowerCase()}.md` || \n' +
    '                 file.toLowerCase() === fileLink.toLowerCase();\n' +
    '        });\n' +
    '        \n' +
    '        if (exactMatch) {\n' +
    "          foundFilePath = exactMatch.replace(/\\.md$/, '');\n" +
    '        } else {\n' +
    '          // Try to find by a'... 3165 more characters
}
